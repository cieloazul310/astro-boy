{
  "name": "astro-boy",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "panda": "panda",
    "eslint": "eslint src --ext .js,.ts,.astro --fix",
    "format": "prettier \"src/**/*.{js,ts,astro}\" --write",
    "lint-staged": "lint-staged",
    "prepare": "git config --local core.hooksPath .githooks & panda codegen"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "^3.2.0",
    "astro": "^3.2.2",
    "astro-icon": "^0.8.1",
    "microcms-js-sdk": "^2.6.1",
    "rehype": "^13.0.1",
    "rehype-class-names": "^1.0.14",
    "string-strip-html": "^13.4.3"
  },
  "devDependencies": {
    "@pandacss/astro": "^0.15.5",
    "@pandacss/dev": "^0.15.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-import": "^2.28.1",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts,astro}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
